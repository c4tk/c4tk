---
component: event-header
---
<!-- Header -->
<header id="header" class="alt">
  <h1 id="logo"><a href="{{relative page.dest "dist/index.html"}}">{{ site.title }}</a></h1>
  <nav id="nav">
    <ul>

      {{#if homealtLanguage}}
          <li class="current"><a href="index.html">{{ this.homealtLanguage}}</a></li>
      {{else}}
          <li class="current"><a href="index.html">Home</a></li>
      {{/if}}

      <li class="submenu">
        <a href="">Menu</a>
        <ul>
          {{#each sections}}
            {{#isnt this.title '(hidden)'}}
            <li>
              <a href="#{{this.path}}">{{ this.title }}</a>
            </li>
            {{/isnt}}
          {{/each}}
          <li class="submenu"><a href="#">Events</a>
            <ul>
              {{#each pages}}
                {{#is data.event "latest"}}
                <li>
                  {{#is data.title "Jakarta 2016"}}
                    <a href="http://codeforthekingdom.id">{{data.title}}</a>

                  {{else }}
                    {{#is data.title  "Los Angeles 2015"}}
                      <a href="http://www.codeforthekingdomla.com">{{data.title}}</a>
                    {{else }}
                       {{#is data.title  "Nashville 2016"}}
                         <a href="http://codeforthekingdomnashville.org">{{data.title}}</a>
                       {{else }}
                           {{#is data.title  "Chicago 2017"}}
                             <a href="https://www.chic4tk.tech">{{data.title}}</a>
                         {{else }}
                            {{#is data.title  "London 2017"}}
                               <a href="http://kingdomcode.uk/build/">{{data.title}}</a>
                            {{else }}
                             {{#is data.title  "London"}}
                               <a href="http://kingdomcode.uk">{{data.title}}</a>
                              {{else }}


                               <a href="{{relative dest this.dest}}">{{data.title}}</a>
                             {{/is}}
                           {{/is}}
                       {{/is}}
                      {{/is}}

                    {{/is}}
                  {{/is}}


                </li>
                {{/is}}
              {{/each}}
            </ul>
          </li>
          {{#each pages}}
            {{#is data.main true}}
            <li>
                    <a href="{{relative dest this.dest}}">{{ data.title }}</a>
            </li>
            {{/is}}
          {{/each}}
        </ul>
      </li>


      {{#if registrationaltLanguage}}
       <li><a href="{{registrationLink}}" class="button special">{{registrationaltLanguage}}</a></li>

      {{else}}
        <li><a href="{{registrationLink}}" class="button special">Sign Up</a></li>

      {{/if}}


      {{#if altLanguage}}
        <li>
          <a href="{{altLanguageUrl}}" class="button">{{altLanguage}}</a>
        </li>
      {{/if}}
    </ul>
  </nav>
</header>
